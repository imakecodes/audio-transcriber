generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Transcription {
  id            String   @id @default(uuid())
  filename      String
  originalName  String
  name          String?
  description   String?
  text          String
  formattedText String?
  createdAt     DateTime @default(now())
}
